INT2STR:        
        MOV #0,-(SP)      ; Запись в стек значений степени 10
        MOV #12,-(SP)     ; последний ноль - это стоп алгоритму
        MOV #144,-(SP)
        MOV #1750,-(SP)
        MOV #23420,-(SP)
                
M1:
        MOV     (SP)+,R2  ; Извлекаем степень 10
        CMP	R2,#0     ; Если конец, то идем к последнему шагу
        BEQ     D1
        MOV	#0,R3     ; Счетчик текущего разряда
C1:     CMP	R0,R2     ; Если текущее число меньше разряда, то сразу в запись
        BLT     S1
        SUB	R2,R0     ; ВЫчитаем текущую степень из числа
        INC	R3        ; Увеличиваем разряд
        JMP     C1

S1:     ADD	#60,R3    ; Преобразуем число в десятичный вид и пишем в буфер
        MOVB	R3,(R1)+

        JMP     M1

D1:     ADD	#60,R0    ; Для единицы, её просто нужно перевести и записать
        MOVB	R0,(R1)+
        
        MOVB	#0,(R1)   ; Ставим терминальный ноль

        RTS PC
